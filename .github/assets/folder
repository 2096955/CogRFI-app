def create_header():
    col1, col2, col3 = st.columns([1, 3, 1])
    
    with col2:
        st.image("assets/cognizant-logo.jpg", width=150)  # Adjust path and width as needed
        st.markdown("""
            <h1 style="margin-top: -10px; color: #0033A1;">RFI Assistant</h1>
        """, unsafe_allow_html=True)
    
    return col1, col2, col3